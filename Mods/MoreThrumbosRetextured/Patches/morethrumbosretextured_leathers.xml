<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<Operation Class="PatchOperationAddModExtension">
	<xpath>Defs/ThingDef[defName="FluffyThrumboLeather"]</xpath>
	<value>
		<li Class="CyanobotsLeather.DME_LeatherExtension">
			<hideDescription>The unprocessed pelt of a wooly thrumbo. Luxurious, durable and valuable even before tanning.</hideDescription>
			<shornEquivalent>CYB_Leather_Thick</shornEquivalent>
			<tanFromWoolHide>true</tanFromWoolHide>
		</li>
	</value>
</Operation>

<Operation Class="PatchOperationAddModExtension">
	<xpath>Defs/ThingDef[defName="MiniThrumboLeather"]</xpath>
	<value>
		<li Class="CyanobotsLeather.DME_LeatherExtension">
			<hideDescription>The unprocessed pelt of a mini thrumbo. Luxurious, durable and valuable even before tanning.</hideDescription>
			<shornEquivalent>CYB_Leather_Thick</shornEquivalent>
			<tanFromWoolHide>true</tanFromWoolHide>
		</li>
	</value>
</Operation>

</Patch>