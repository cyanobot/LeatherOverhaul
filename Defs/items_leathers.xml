	<ThingDef ParentName="LeatherBase">
		<defName>CYB_Leather_Wool</defName>
		<label>woolskin</label>
		<description>The tanned hide of a wooly animal, still bearing an insulating layer of wool.</description>
		<graphicData>
		  <color>(220,220,220)</color>
		</graphicData>
		<statBases>
			<MaxHitPoints>120</MaxHitPoints>
			<MarketValue>2.3</MarketValue>
			<StuffPower_Armor_Sharp>0.76</StuffPower_Armor_Sharp>
			<StuffPower_Armor_Blunt>0.29</StuffPower_Armor_Blunt>
			<StuffPower_Armor_Heat>1.5</StuffPower_Armor_Heat>
			<StuffPower_Insulation_Cold>25</StuffPower_Insulation_Cold>
			<StuffPower_Insulation_Heat>14</StuffPower_Insulation_Heat>
			<Flammability>1.0</Flammability>
		</statBases>
		<stuffProps>
		  <color>(220,220,220)</color>
		  <commonality>0.08</commonality>
		  <!--
		  <categories>
			<li>CYB_Furry</li>
		  </categories>
		  -->
